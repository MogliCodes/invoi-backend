{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "node"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:node/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
        "node/no-unsupported-features/es-syntax": "off"
      }
  }